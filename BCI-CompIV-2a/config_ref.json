{
  "description": "",
  "indiv": {
    "net": {
      "class": "EEGNetBaseline",
      "params": {
        "F1": 8,
        "D": 2,
        "F2": 16,
        "C": 22,
        "T": 1125,
        "N": 4,
        "p_dropout": 0.5,
        "activation": "relu",
        "dropout_type": "TimeDropout2D"
      },
      "pretrained": null
    },
    "loss_function": {
      "class": "CrossEntropyLoss",
      "params": {}
    }
  },
  "treat": {
    "thermostat": {
      "params": {
        "noise_schemes": {},
        "bindings": []
      }
    },
    "optimizer": {
      "class": "Adam",
      "params": {"lr": 1e-3}
    },
    "lr_scheduler": {
        "class": "StepLR",
        "params": {"step_size": 1000, "gamma": 1}
    },
    "data": {
      "subject": 1,
      "fs": 250,
      "t1_factor": 1.5,
      "t2_factor": 6.0,
      "filter": {
        "type": "none",
        "order": 4,
        "fc": 4.0,
        "fc_low": 4.0,
        "fc_high": 40.0
      },
      "valid_fraction": 0.1,
      "use_test_as_valid": true,
      "bs_train": 32,
      "bs_valid": 32
    },
    "max_epoch": 500
  },
  "protocol": {
    "update_metric": "valid_metric",
    "metric_period": 1
  }
}
